# Credentials file for SFTP authentication
# Copy this file to ~/.config/save_audio_stream/credentials and update the passwords
#
# Format:
# [profile_name]
# password = "your_password_here"

[default]
password = "your_sftp_password_here"

[production]
password = "production_sftp_password"

[staging]
password = "staging_sftp_password"
